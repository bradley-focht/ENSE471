		<FlowDocumentReader Name="flowStats" ViewingMode="Page" MinZoom="100" IsFindEnabled ="False" IsScrollViewEnabled="True" IsTwoPageViewEnabled="False"  UseLayoutRounding="True" DockPanel.Dock="Right">
			<FlowDocumentReader.Resources>
				<Style TargetType="TableCell">
					<Setter Property="TextAlignment" Value="Center"/>
					<Setter Property="FontSize" Value="12" />
					<Setter Property="FontFamily" Value="Arial" />
					<Setter Property="Foreground" Value="White" />
					
				</Style>
			</FlowDocumentReader.Resources>
			<FlowDocument Name="flowDoc">
				
   
			</FlowDocument>
		</FlowDocumentReader>

		</DockPanel>

		<DockPanel Name="dckLogin" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="Collapsed">
		 
			<StackPanel Orientation="Vertical">
				<Image Source="logo.gif" Height="100" Width="100" />
				<TextBlock  Foreground="Wheat" Margin="5" FontSize="20">Canadian Education Strategy Program</TextBlock>
				<StackPanel Orientation="Horizontal">
					<StackPanel Orientation="Vertical" Margin="10" >
						<TextBlock Foreground="Wheat" Margin="5" FontSize="15" HorizontalAlignment="Right">Username</TextBlock>
						<TextBlock  Foreground="Wheat" Margin="5" FontSize="15" HorizontalAlignment="Right">Password</TextBlock>
					</StackPanel>
					<StackPanel Orientation="Vertical" Margin="10" >
						<TextBox Name="txtUsername" Width="200" Margin="5" HorizontalAlignment="Left"/>
						<TextBox Name="txtPassword" Width="200" Margin="5" HorizontalAlignment="Left"/>
						<Button  Name="btnLogin" HorizontalAlignment="Left" Margin="5" Content="Login" Width="100" Click="screenChange"  />
						<TextBlock HorizontalAlignment="Left" Foreground="Wheat" Margin="5">Can't login? Call Sean at 123-456.</TextBlock>
						<TextBlock HorizontalAlignment="Left" Foreground="Wheat" Margin="5">Can't get ahold of him? Try his mom at 321-7654</TextBlock>
					</StackPanel>
					
				</StackPanel>
 
			</StackPanel>

		</DockPanel>
		<DockPanel x:Name="dckJobs" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="Collapsed">
			<Border DockPanel.Dock="Left" BorderBrush="#0A2846" Background="#051423" BorderThickness="2, 0, 2, 2" >
				<StackPanel Orientation="Vertical" Name="navScreen">
					<StackPanel.Resources>
						<Style TargetType="{x:Type Button}">
							<Setter Property="Foreground" Value="White"/>
							<Setter Property="Padding" Value="5"/>
							<Setter Property="BorderBrush" Value="Transparent"/>
							<Setter Property="Background" Value="#0A2846" />
							<Setter Property="HorizontalContentAlignment" Value="Left"/>
						</Style>
					</StackPanel.Resources>
					<Button>Alberta</Button>
					<Button>British Columbia</Button>
					<Button>Manitoba</Button>
					<Button>New Brunswick</Button>
					<Button>Newfoundland</Button>
					<Button>Nova Scotia</Button>
					<Button>Ontario</Button>
					<Button>Prince Edward Island</Button>
					<Button>Quebec</Button>
					<Button Background="#1450C8">Saskatchewan</Button>
				</StackPanel>
			</Border>
			<Grid x:Name="JobsGrid" Margin="30" >
				<Grid.Resources>
					<Style TargetType="{x:Type Label}">
						<Setter Property="Foreground" Value="White"/>
					</Style>
					<Style TargetType="{x:Type ComboBox}">
						<Setter Property="Height" Value="20"/>
						<Setter Property="VerticalAlignment" Value="Center"/>
					</Style>
					<Style TargetType="{x:Type TextBox}">
						<Setter Property="Height" Value="20"/>
						<Setter Property="VerticalAlignment" Value="Center"/>
					</Style>
					<Style TargetType="{x:Type RadioButton}">
						<Setter Property="Margin" Value="0,10,0,0"/>
					</Style>
					<Style TargetType="{x:Type Button}">
						<Setter Property="Foreground" Value="White"/>
						<Setter Property="Padding" Value="5"/>
						<Setter Property="BorderBrush" Value="Transparent"/>
						<Setter Property="Background" Value="#0A2846" />
					</Style>
				</Grid.Resources>
				<Grid.ColumnDefinitions>
					<ColumnDefinition MaxWidth="100" MinWidth="70"/>
					<ColumnDefinition MaxWidth="200"/>
				</Grid.ColumnDefinitions>
				<Grid.RowDefinitions>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="30"/>
					<RowDefinition Height="100"/>
					<RowDefinition Height="30"/>
				</Grid.RowDefinitions>
				<Label Grid.Column="0" Grid.Row="0">Field:</Label>
				<ComboBox Width="100" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Left"/>
				<ComboBox Width="100" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left"/>

				<Label Grid.Column="0" Grid.Row="2">Projections:</Label>
				<ComboBox Width="100" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Left"/>

				<Label Grid.Column="0" Grid.Row="3">Increase</Label>
				<RadioButton Grid.Column="1" Grid.Row="3" Name="up" GroupName="upDown"/>

				<Label  Grid.Column="0" Grid.Row="4">Decrease</Label>
				<RadioButton Grid.Column="1" Grid.Row="4" Name="down" GroupName="upDown"/>

				<Label Grid.Column="0" Grid.Row="5">By:</Label>
				<TextBox Grid.Column="1" Grid.Row="5" Width="100" HorizontalAlignment="Left"/>

				<Label Grid.Column="0" Grid.Row="6">Current:</Label>

				<Label Grid.Column="0" Grid.Row="7">Employed:</Label>
				<TextBox Grid.Column="1" Grid.Row="7" Width="100" HorizontalAlignment="Left"/>

				<Label Grid.Column="0" Grid.Row="8">Openings:</Label>
				<TextBox Grid.Column="1" Grid.Row="8" Width="100" HorizontalAlignment="Left"/>

				<Label Grid.Column="0" Grid.Row="9">Salary:</Label>
				<TextBox Grid.Column="1" Grid.Row="9" Width="100" HorizontalAlignment="Left"/>

				<Label Grid.Column="0" Grid.Row="10">Comments:</Label>

				<TextBox Grid.Column="0" Grid.Row="11" Grid.ColumnSpan="2" Margin="20,0,0,0" HorizontalAlignment="Left" Width="180" Height="100" TextWrapping="Wrap" AcceptsReturn="True"/>

				<Button Grid.Column="0" Grid.Row="12">Accept</Button>
				<Button Grid.Column="1" Grid.Row="12" HorizontalAlignment="Left" Width="100">Cancel</Button>
			</Grid>
		</DockPanel>
		<DockPanel x:Name="dckEducation" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="Collapsed">
		</DockPanel>
	</DockPanel>
</Window>
